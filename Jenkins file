pipeline {
    agent any

    stages {
        stage('git clone') {
            steps {
               git branch: 'main', url: 'https://github.com/varunchi/gitlearn.git'
            }
        }
        stage('hello') {
            steps {
               eco 'hii'
            }
        }
        stage('maven') {
            steps {
               sh 'mvn clean package'
               
            }
        }
    }
}
